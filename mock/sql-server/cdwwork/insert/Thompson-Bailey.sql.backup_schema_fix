-- =====================================================
-- SECTION 9: ALLERGIES - Allergy.PatientAllergy
-- =====================================================
-- Bailey Thompson: 2 drug allergies
-- Allergy 1: Penicillin (documented 2010, rash reaction)
-- Allergy 2: Morphine (documented 2016, nausea/vomiting after suicide attempt)
-- =====================================================

PRINT '';
PRINT 'Section 9: Allergies - Allergy.PatientAllergy';
PRINT '  Inserting Bailey Thompson drug allergies (2 total)';
GO

INSERT INTO Allergy.PatientAllergy
(
    PatientAllergySID,
    PatientSID,
    AllergenSID,
    AllergyType,
    ObservedHistorical,
    AllergyEnteredDateTime,
    AllergyEnteredVistaErrorDate,
    AllergyEnteredDateTimeTransformSID,
    VerificationStatus,
    VerificationDateTime,
    VerificationVistaErrorDate,
    VerificationDateTimeTransformSID,
    Sta3n
)
VALUES
-- Allergy 1: Penicillin
(
    NULL,                           -- PatientAllergySID (IDENTITY)
    2001,                           -- PatientSID
    (SELECT AllergenSID FROM Dim.Allergen WHERE AllergenName LIKE '%PENICILLIN%'),
    'DRUG',                         -- AllergyType
    'HISTORICAL',                   -- ObservedHistorical (patient reported during enrollment)
    '2010-06-15',                   -- AllergyEnteredDateTime (enrollment date)
    NULL,                           -- AllergyEnteredVistaErrorDate
    NULL,                           -- AllergyEnteredDateTimeTransformSID
    'VERIFIED',                     -- VerificationStatus
    '2010-06-15',                   -- VerificationDateTime
    NULL,                           -- VerificationVistaErrorDate
    NULL,                           -- VerificationDateTimeTransformSID
    516                             -- Sta3n
),
-- Allergy 2: Morphine
(
    NULL,
    2001,
    (SELECT AllergenSID FROM Dim.Allergen WHERE AllergenName LIKE '%MORPHINE%'),
    'DRUG',
    'OBSERVED',                     -- ObservedHistorical (observed during hospitalization)
    '2016-09-13',                   -- AllergyEnteredDateTime (day after suicide attempt admission)
    NULL,
    NULL,
    'VERIFIED',
    '2016-09-13',
    NULL,
    NULL,
    516
);
GO

PRINT '  Bailey Thompson allergies inserted (2 allergies: Penicillin, Morphine)';
GO

-- =====================================================
-- SECTION 10: ALLERGIES - Allergy.PatientAllergyReaction
-- =====================================================
